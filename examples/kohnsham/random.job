#!/bin/bash
#SBATCH -n 1 -N 1

module load venv/qimpy

for n_bulk in 0.4 0.5 0.6 0.7 0.8; do
	export n_bulk
	for L in 1 2 5 10; do
		export L
		for sigma in 0.1 0.2 0.5 1.0 2.0; do
			export sigma
			for i_seed in {1..10}; do
				export seed=$((seed + 1))
				python gen_data_electronic.py random.yaml
			done
		done
	done
done
